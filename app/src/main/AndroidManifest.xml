<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vanapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity"
            android:label="@string/titulo_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainSplashActivity"></activity>
        <activity android:name=".MenuInicialActivity"
            android:label="@string/titulo_menu_principal">
        </activity>
        <activity
            android:name=".PresentationLayerUsuarios.UsuariosActivity"
            android:label="@string/titulo_usuarios"
            android:parentActivityName=".ColorPickerTestActivity">
        </activity>
        <activity
            android:name=".PresentationLayerUsuarios.UsuarioExistenteActivity"
            android:label="@string/titulo_usuario_detalle_editar"
            android:parentActivityName=".PresentationLayerUsuarios.UsuariosActivity">
        </activity>
        <activity
            android:name=".ColorPickerTestActivity"
            android:label="VanApp - Test Activity"
            android:parentActivityName=".PresentationLayerUsuarios.UsuariosActivity">
        </activity>
        <activity
            android:name=".PresentationLayerUsuarios.UsuarioAltaActivity"
            android:label="@string/titulo_usuario_detalle_consulta"
            android:parentActivityName=".PresentationLayerUsuarios.UsuariosActivity"></activity>
        <activity android:name=".TestUsuariosActivity"></activity>
        <activity android:name=".DatabaseTestActivity"></activity>
    </application>

</manifest>