<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vanapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ColorPickerTestActivity"
            android:parentActivityName=".PresentationLayerUsuarios.UsuariosActivity"
            android:label="VanApp - Test Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PresentationLayerUsuarios.UsuarioAltaActivity"
            android:parentActivityName=".PresentationLayerUsuarios.UsuariosActivity"
            android:label="@string/titulo_usuario_detalle_consulta">
        </activity>
        <activity
            android:name=".PresentationLayerUsuarios.UsuarioExistenteActivity"
            android:parentActivityName=".PresentationLayerUsuarios.UsuariosActivity"
            android:label="@string/titulo_usuario_detalle_editar">
        </activity>
        <activity
            android:name=".PresentationLayerUsuarios.UsuariosActivity"
            android:parentActivityName=".ColorPickerTestActivity"
            android:label="@string/titulo_usuarios">
        </activity>
        <activity android:name=".TestUsuariosActivity">
        </activity>
        <activity android:name=".DatabaseTestActivity">
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".MainSplashActivity">
        </activity>
    </application>

</manifest>